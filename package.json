  "scripts": {
    "build": "rollup -c",
    "dev": "cp ./envs/env.dev ./.env && node scripts/start.js",
    "start": "node scripts/start.js",
    "preprod": "cp ./envs/env.pro ./.env",
    "prod": "node scripts/build.js",
    "build-for-react": "yarn build  && yarn publish",
    "build:bundle": "webpack --config webpack.config.js",
    "upload": "aws s3 cp ./dist/jdresv.min.js s3://stayjanda.modules.public/resvModule/1.2.0/resvModule.js && aws s3 cp ./build/static/css/main.min.css s3://stayjanda.modules.public/resvModule/1.0.0/resvModule.css ",
    "upload:stable": "aws s3 cp ./dist/jdresv.min.js s3://stayjanda.modules.public/resvModule/booking--stable/resvModule.js && aws s3 cp ./build/static/css/main.min.css s3://stayjanda.modules.public/resvModule/booking--stable/resvModule.css ",
    "upload:latest": "aws s3 cp ./dist/jdresv.min.js s3://stayjanda.modules.public/resvModule/booking--latest/resvModule.js && aws s3 cp ./build/static/css/main.min.css s3://stayjanda.modules.public/resvModule/booking--latest/resvModule.css ",
    "test": "react-app-rewired test --env=jsdom",
    "cjs": "yarn prod && yarn build:bundle && yarn upload",
    "cjs:latest": "yarn prod && yarn build:bundle && yarn upload:latest",
    "cjs:stable": "yarn prod && yarn build:bundle && yarn upload:stable",
    "precodegen": "apollo schema:download --endpoint=https://temp-booking.stayjanda.cloud/graphql",
    "codegen": "apollo codegen:generate src/types/api.ts --queries=src/apollo/queries.ts --addTypename --localSchemaFile=schema.json --target=typescript --outputFlat"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "**/react": "16.13.0",
    "**/react-dom": "16.13.0"
  },
  "peerDependencies": {
    "node-sass": "^4.13.1",
    "react": "^16",
    "react-day-picker": "^7.4.8",
    "react-dom": "^16"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/apollo-upload-client": "^8.1.3",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.0",
    "@types/jquery": "^3.3.35",
    "@types/lodash": "^4.14.154",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-slick": "^0.23.4",
    "@types/react-window-size": "^1.2.0",
    "@types/styled-components": "^5.1.0",
    "customize-cra": "^0.9.1",
    "node-sass": "^4.13.1",
    "react-app-rewired": "^2.1.5",
    "rollup": "^1.27.13",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "~3.9.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}