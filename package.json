	"scripts": {
		"start": "react-app-rewired start",
		"prebuild": "cp ./envs/env.pro ./.env",
		"build": "yarn build:react && yarn build:bundle",
		"build:react": "react-app-rewired build",
		"build:bundle": "webpack --config webpack.config.js",
		"upload": "aws s3 cp ./dist/resvModule.min.js s3://stayjanda.modules.public/resvModule/1.0.0/resvMoudle.js",
		"test": "react-app-rewired test --env=jsdom",
		"eject": "react-scripts eject",
		"precodegen": "apollo schema:download --endpoint=https://temp-booking.stayjanda.cloud/graphql",
		"codegen": "apollo codegen:generate src/types/api.d.ts --queries=src/apollo/queries.ts --addTypename --localSchemaFile=schema.json --target=typescript --outputFlat"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [ ">0.2%", "not dead", "not op_mini all" ],
		"development": [ "last 1 chrome version", "last 1 firefox version", "last 1 safari version" ]
	},
	"resolutions": {
		"**/react": "16.13.0",
		"**/react-dom": "16.13.0"
	},
	"devDependencies": {
		"customize-cra": "^0.9.1",
